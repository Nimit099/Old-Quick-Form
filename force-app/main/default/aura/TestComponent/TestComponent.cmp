                                    <lightning:button variant="brand-outline" label="Remove" onclick="{!c.removeBackground}" title="titleName" class="slds-m-top--medium" disabled="{!v.storeFormbgId == ''}" />
                                    <lightning:combobox label="Background size" class="mt-3" value="{!v.formbackSize}" options="{!v.optformbackSize}" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Background position" value="{!v.formbackpagePostion}" options="{! v.optformbackpagePostion}" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Background repeat" value="{!v.formbackpageRepeat}" options="{! v.optformbackpageRepeat}" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Background fix position" value="{!v.formbackgroundPagefixposition}" options="{! v.optformbackgroundPagefixposition}" onchange="{!c.handleCss}" class="slds-m-bottom_medium" />
                                </div>

                                <button class="accordion" onclick="{!c.designaccordian}">Page</button>
