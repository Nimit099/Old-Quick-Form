                                    <lightning:combobox label="Font weight" value="{!v.labelfontweight}" options="{!v.optlabelfontweight}" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font style" value="{!v.labelfontstyle}" options="{!v.optlabelfontstyle}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" value="{!v.labelfontsize}" label="Font size (px)" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font line height" value="{!v.labelineheight}" options="{! v.optlabelineheight}" onchange="{!c.handleCss}" />
                                    <lightning:input type="color" value="{!v.labelcolor}" label="Color" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" value="{!v.labeltopmargin}" label="Top margin (px)" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" value="{!v.labelbottommargin}" label="Bottom margin (px)" onchange="{!c.handleCss}" class="slds-m-bottom_medium" />
                                </div>

                                <button class="accordion" onclick="{!c.designaccordian}" style="display: none;">Instrunction</button>
                                <button class="accordion" onclick="{!c.designaccordian}">Input</button>
                                <div class="panel">
                                    <lightning:input type="color" label="Background" value="{!v.bgInput}" onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Border" value="{!v.borderInput}" onchange="{!c.handleCss}" />
                                    <lightning:combobox name="Border style" label="Border style" value="{!v.borderStyle}" options="{! v.optborderStyle}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Border thickness (px)" value="{!v.borderWidth}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Border radius (px)" value="{!v.borderRadius}" onchange="{!c.handleCss}" />
                                    <lightning:combobox name="Font" label="Font" options="{!v.optinputfontfamily}" value="{!v.inputfontfamily}" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font weight" value="{!v.inputfontweight}" options="{! v.optinputfontweight }" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font style" value="{!v.inputfontstyle}" options="{! v.optinputfontstyle}" onchange="{!c.handleCss}" />
                                    <lightning:input type="number" label="Font size (px)" value='{!v.inputfontsize}' onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font line height" value="{!v.inputlineheight}" options="{! v.optinputlineheight}" onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Text color" value='{!v.bordertextcolor}' onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Horizontal padding (px)" value='{!v.inputHpadding}' onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Vertical padding (px)" value='{!v.inputVpadding}' onchange="{!c.handleCss}" class="slds-m-bottom_medium" />
                                </div>


                                <button class="accordion" onclick="{!c.designaccordian}">Button</button>
                                <div class="panel">
                                    <lightning:combobox label="Alignment" value='{!v.btnJustify}' options="{!v.optbtnJustify}" onchange="{!c.handleCss}" class="slds-m-bottom_medium" />

                                    <lightning:input type="color" label="Text Color" value="{!v.btnTextColor}" onchange="{!c.handleCss}" />


                                    <lightning:input type="color" label="Background Color" value="{!v.btnBgColor}" onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Border color" value="{!v.btncolor}" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Border style" value="{!v.btnborderstyle}" options="{! v.optbtnborderstyle }" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Border thickness (px)" value="{!v.btnborderwidth}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Border radius (px)" onchange="{!c.handleCss}" value="{!v.btnborderradius}" />
                                    <lightning:combobox label="Font" value="{!v.buttonfontfamily}" options="{! v.optbuttonfontfamily }" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font weight" value="{!v.buttonfontweight}" options="{! v.optbuttonfontweight }" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Font style" value="{!v.buttonfontstyle}" options="{! v.optbuttonfontstyle }" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" value="{!v.buttonfontsize}" label="Font size (px)" onchange="{!c.handleCss}" />

                                    <lightning:input type="text" label="Width (px)" onchange="{!c.handleCss}" value="{!v.btnWidth}" />
                                    <lightning:input type="text" label="Height (px)" onchange="{!c.handleCss}" value="{!v.btnHeight}" />
                                    <lightning:input type="text" label="Horizontal Padding (px)" onchange="{!c.handleCss}" value="{!v.btnHorizontalPadding}" />
                                    <lightning:input type="text" label="Vertical Padding (px)" onchange="{!c.handleCss}" value="{!v.btnVerticalPadding}" class="slds-m-bottom_medium" />


                                </div>




                                <button class="accordion" onclick="{!c.designaccordian}">Field Hover</button>
                                <div class="panel">
                                    <lightning:input type="color" label="Input background" value='{!v.fieldhoverbg}' onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Input border" value='{!v.fieldhoverborderColor}' onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Input text color" value='{!v.fieldhovercolor}' onchange="{!c.handleCss}" class="hoverTextColor" />
                                    <lightning:input type="color" label="Label color" value="{!v.hoverlabelcolor}" onchange="{!c.handleCss}" id="label_popup" class="hoverLabelColor slds-m-bottom_medium" />
                                </div>

                                <button class="accordion" onclick="{!c.designaccordian}">Field Focus</button>
