                                            <!--Modal/Popup Box Body Starts here-->
                                            <div class="slds-modal__content content " id="modal-content-id-1">
                                                <lightning:button variant="bare" iconName="utility:close" onclick="{!c.hideModel5 }" />
                                                <iframe aura:id="vframe" class="iframe" src="{!v.publishurl}" style="width:55rem ; height:100%" />
                                            </div>
                                        </div>
                                    </section>
                                    <div class="slds-backdrop slds-backdrop_open">
                                    </div>
                                </aura:if>
                                <aura:set attribute="else">
                                    <div style="width:100%;">
                                        <div style="margin: auto; text_align:center; justify-contant:center; margin : 11% 0;">
                                            <img style="hight:15%; width:40%; margin:0 30% 0 30%;" src="{!$Resource.forcesite}" alt="Force.com Site" />
                                            <h1 style="font-size:30px; text-align:center; font-size:28px;">Your Force.com site is currently deactivated</h1>
                                            <h2 style=" font-size:16px; text-align:center;">Please ensure that site iS active at the moment from here.</h2>
                                        </div>
                                    </div>
                                </aura:set>
                            </aura:if>
                        </div>
                    </div>
                </lightning:tab>
            </lightning:tabset>
        </div>
        <!-- end vertical tab div  -->

        <!-- Validation bar-->
        <div class="fieldDiv2 ">
            <c:FieldAttributeValidation />
        </div>
        <!-- End Validation bar -->

        <!-- Main Form Div -->
        <div style="display: flex; flex-direction:row; position:relative; width:100%">

            <div class="addFormTitle">
                <div class="InnerFormtitle">
                    <div class="formName">
                        <p> {!v.FormPageFieldValueWrapper.formName.Title__c}</p>
                    </div>
                    <div class="formButtons">
                        <lightning:button label="Edit" iconName="utility:edit" iconPosition="left" onclick="{!c.openValidationBar}" />
                        <lightning:button value="new page add" label="Add Page" iconPosition="left" iconName="utility:record_create" onclick="{!c.handleSelect}" name="{!'form:::'+v.FormId}" />
                    </div>


                </div>
            </div>



            <div class="fieldDiv1 slds-col  slds-small-size_6-of-12  slds-size_1-of-1 slds-medium-size_8-of-12 slds-large-size_9-of-12" style="padding:5%; width:100%">

                <div class="myform" id="mainformid">
                    <div class="formtitle"></div>
                    <aura:iteration items="{!v.FormPageFieldValueWrapper.PageWrapperList}" var="page" indexVar="index">
                        <div class="page" id="{!page.PageObj.Id}">
                            <div class="pagetitle">
                                <div style="display: flex;align-items: center;">
                                    <div>
                                        <aura:if isTrue="{!page.PageObj.Title__c == null}">
                                            <aura:set attribute="else">
                                                <div class="pagetitle1">{!page.PageObj.Title__c}</div>
                                            </aura:set>
                                        </aura:if>
                                        <aura:if isTrue="{!page.PageObj.Sub_Title__c == null}">
                                            <aura:set attribute="else">
                                                <div class="subpagetitle">{!page.PageObj.Sub_Title__c}</div>
                                            </aura:set>
                                        </aura:if>
                                    </div>
                                </div>
                                <lightning:buttonMenu class="pageButtonMenu" alternativeText="Show menu" onselect="{! c.handleSelectPage }" name="{!'page:::'+page.PageObj.Id}">
                                    <lightning:menuItem aura:id="menuItems" value="edit" label="Edit Page" prefixIconName="utility:edit" />
                                    <lightning:menuItem aura:id="{!page.PageObj.Id}" value="delete" label="Delete Page" prefixIconName="utility:delete" />
                                </lightning:buttonMenu>
                            </div>
                            <div class="example-dropzone" ondragover="{!c.onDragOver}" ondrop="{!c.onDrop}" ondragleave="{!c.dragLeave}" ondragenter="{!c.dragEnter}">
                                <aura:iteration items="{!page.FieldWrapperList}" var="fieldList">
                                    <div id="{!page.PageObj.Id}" name="{!'field:::'+fieldList.FieldObj.Id}" class="field" draggable="true" data-record="{!fieldList.FieldObj.Id}" data-ref="inner" ondragstart="{!c.onDragStart}" onclick="{!c.handleid}">
                                        <c:quickformfieldcomponent1 fieldValue="" tView="{!fieldList.FieldObj.Data_Record_Id__c}" Compview="Full" disableField="true" FieldAttribute="{!fieldList.FieldAttObj}" FieldAttributeValue="{!fieldList.FieldValueObj}" FieldId="{!fieldList.FieldObj.Id}"
                                            PageId="{!page.PageObj.Id}" /><br />
                                    </div>
                                </aura:iteration>
                            </div>
                            <div class="footer">

                                <aura:if isTrue="{!index == 0}">

                                    <lightning:button class="nextbutton " variant="brand" label="Next" title="Next" disabled="true" onclick="{!c.onaddpage1}" />
                                    <aura:set attribute="else">
                                        <aura:if isTrue="{!index != v.FormPageFieldValueWrapper.PageWrapperList.length - 1}">
                                            <div class="previousnextbutton slds-m-bottom_large">
                                                <lightning:button aura:id="previousButton" variant="brand" label="Previous" title="Previous" disabled="true" onclick="{!c.onaddpage1}" class="btn" />
                                                <lightning:button aura:id="nextButton" variant="brand" label="Next" title="Next" disabled="true" onclick="{!c.onaddpage1}" class="btn" />
                                            </div>
                                            <aura:set attribute="else">
                                                <div class="previoussubmitbutton slds-m-bottom_large">
                                                    <lightning:button variant="brand" label="Previous" title="Previous" disabled="true" onclick="{!c.onaddpage1}" class="btn" />
                                                    <lightning:button variant="brand" label="Submit" title="Submit" disabled="true" onclick="{!c.onaddpage1}" class="btn" />
                                                </div>
                                            </aura:set>
                                        </aura:if>
                                    </aura:set>
                                </aura:if>
                            </div>
                        </div>
                        <div style="padding:3% 0"></div>
                    </aura:iteration>
                </div>
            </div>
            <!-- End Main form div -->
        </div>
    </div>
    <!-- end of main component -->
</aura:component>