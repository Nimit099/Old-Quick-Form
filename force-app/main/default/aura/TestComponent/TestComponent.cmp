                                    <img src="{!'/servlet/servlet.FileDownload?file=' + v.storebgId}" alt="" style="display: block;  margin: auto;   margin-top: 10px;" />
                                    <lightning:input type="text" value='{!v.storebgId}' style="display:none" />
                                    <lightning:button variant="brand-outline" label="Remove" onclick="{!c.removePageBackground}" title="titleName" class="slds-m-top--medium" disabled="{!v.storebgId == ''}" />
                                    <lightning:combobox label="Background size" class="mt-3" value="{!v.backSize}" options="{!v.optbackSize}" onchange="{!c.handleCss}" />
