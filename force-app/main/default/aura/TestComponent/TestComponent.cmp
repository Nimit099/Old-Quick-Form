    <!--=========================[ page Modal ] ========================= -->
    <div>
        <section aura:id="Modal" role="dialog" tabindex="-1" aria-labelledby="ModalHeading" aria-modal="true" aria-describedby="ModalBody" class="slds-modal">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <lightning:buttonIcon class="slds-modal__close" alternativeText="Close" iconName="utility:close" onclick="{!c.closeModal}" variant="bare-inverse" size="large"></lightning:buttonIcon>
                    <div class="header">
                        <h5 id="ModalHeading">New Page</h5>
                    </div>
                </header>
                <div class="slds-modal__content slds-p-around_medium" id="ModalBody">
                    <div style="float:right;">
                        <lightning:helptext content="To display the page title"></lightning:helptext>
                    </div>
                    <div>
                        <lightning:input label="Page Title " value="{!v.Page.Title__c}" class="validate" />
                    </div>
                    <div style="float:right;">
                        <lightning:helptext content="To display the page subtitle"></lightning:helptext>
                    </div>
                    <div>
                        <lightning:input label="Page Sub-title" value="{!v.Page.Sub_Title__c}" />
                    </div>
                </div>
                <footer class="slds-modal__footer">
                    <div>
                        <lightning:button label="Cancel" title="Cancel Page" onclick="{!c.closeModal}" />
                        <lightning:button variant="brand" label="Save" title="Add Page" onclick="{!c.createPage}" disabled="{!v.Page.Title__c == ''}" />
                    </div>
                </footer>
            </div>
        </section>
        <div aura:id="ModalBackdrop" class="slds-backdrop"></div>
    </div>

    <!-- ========================= [page Modal End ] ========================= -->

    <!-- ========================= [ Add Recipient] Button click  strat=========================-->

    <div>
        <section aura:id="add_Recipient" role="dialog" tabindex="-1" aria-labelledby="ModalHeading" aria-modal="true" aria-describedby="ModalBody" class="slds-modal">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <lightning:buttonIcon class="slds-modal__close" alternativeText="Close" iconName="utility:close" onclick="{!c.cancleAddRecipient}" variant="bare-inverse" size="large">
                    </lightning:buttonIcon>
                    <div class="header">
                        <h2 id="ModalHeading">Add Recipient</h2>
                    </div>
                </header>
                <div class="slds-modal__content slds-p-around_medium" id="ModalBody">
                    <div class="" id="renameTextField">
                        <lightning:input type="email" label="Recipient Email" value="{!v.emailRecipientForList}" required="true" messageWhenValueMissing="Please Fill the Details" />
                    </div>
                </div>
                <footer class="slds-modal__footer">
                    <div class="" style="text-align: center;">
                        <lightning:button label="Cancel" title="Cancel" onclick="{!c.cancleAddRecipient}" />
                        <lightning:button class="" variant="brand" label="Add" onclick="{!c.addRecipient}" />
                    </div>
                </footer>
            </div>
        </section>
        <div aura:id="ModalBackdrop_add_Recipient" class="slds-backdrop"></div>
    </div>

    <!-- ========================= [ Add Recipient ] Button click  finished=========================-->

