                                    <lightning:input type="text" label="Radio margin top (px)" value="{!v.radiomarginright}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Checkbox width (px)" value="{!v.checkwidth}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Checkbox height (px)" value="{!v.checkheight}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" label="Checkbox right margin (px)" value="{!v.checkmargintop}" onchange="{!c.handleCss}" class="slds-m-bottom_medium" />
                                </div>


                            </div>
                        </div>
                    </div>
                </lightning:tab>


                <lightning:tab iconName="utility:notification" title="Notification" onactive="{! c.handleActive }">
                    <div class=" showtabs" style="position:absolute; z-index:11;top:0; overflow:hidden; ">
                        <div class="card emailNotificationDiv">
                            <div class="card-body" style=" padding:2rem">

                                <div class="" style="display:flex; flex-direction:column">
                                    <span style="color: #A4A4A4;margin-bottom:1rem;">RECIPIENTS </span>
                                    <span style="text-transform: uppercase">Email Id</span>

                                    <!-- email box -->

                                    <div class="emailBox">
                                        <div class="slds-truncate">
                                            <aura:iteration items="{!v.ntf_recever_mailid}" var="recipient" indexVar="index">
                                                <div class="emialRecipientIttretion">
                                                    <span style="display:inline;float:left;margin-right: 15px;">
                                                        <option text="{!recipient}"></option>
                                                    </span>
                                                    <span style="display:inline;float:right;margin-right: 15px;">
                                                        <lightning:buttonIcon iconName="utility:delete"
                                                            variant="border-filled" alternativeText="Settings"
                                                            title="Delete Recipient" value="{!index}"
                                                            onclick="{!c.deleteRecipient}" />
                                                    </span>
                                                    <span style="display:inline;float:right;margin-right: 15px;">
                                                        <lightning:buttonIcon iconName="utility:edit"
                                                            variant="border-filled" alternativeText="Edit"
                                                            title="Edit Recipient" value="{!index}"
                                                            onclick="{!c.openEditRecipient}" />
                                                    </span><br />
                                                </div>
                                            </aura:iteration>
                                        </div>
                                    </div>

                                </div>


                                <div style="margin-top: 10px;">
                                    <lightning:button variant="brand" label="New Recipient" title="Add Recipient" onclick="{!c.openAddRecipient}" />
                                </div>

