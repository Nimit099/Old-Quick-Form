                                    <lightning:input type="number" aura:id="input-id-01" value="{!v.formWidth}" label="Form Width (%)" onchange='{!c.handleCss}' />
                                    <lightning:input type="number" name="Header padding" value="{! v.headpadding }" label="Header Padding (%)" onchange='{!c.handleCss}' class="  " />
                                    <lightning:input type="number" name="Footer Padding" value="{! v.footpadding }" label="Footer Padding (%)" onchange='{!c.handleCss}' class="  " />
                                    <lightning:input type="number" name="Left Padding" value="{! v.leftpadding }" label="Left Padding (%)" onchange='{!c.handleCss}' class="  " />
                                    <lightning:input type="number" name="Right padding" value="{! v.rightpadding }" label="Right Padding (%)" onchange='{!c.handleCss}' class="  " />
                                    <lightning:combobox name="Form direction" label="Form direction" options="{! v.options2 }" value='{!v.formdir}' onchange='{!c.handleCss}' class="  " />
                                    <lightning:input type="color" label="Background" name="color" value="{!v.colorpicker}" onchange='{!c.handleCss}' class="slds-color-picker__custom-inputs" />
