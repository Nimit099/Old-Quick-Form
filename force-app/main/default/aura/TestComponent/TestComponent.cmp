                                    <lightning:combobox label="Background repeat" value="{!v.backpageRepeat}" options="{! v.optbackpageRepeat }" onchange="{!c.handleCss}" />
                                    <lightning:combobox label="Background fix position" value="{!v.backgroundPagefixposition}" options="{! v.optBackgroundPagefixposition}" onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Border" name="color" value="{!v.formbordercolor}" onchange='{!c.handleCss}' />
                                    <lightning:combobox label="Border style (px)" value="{!v.formborderStyle}" options="{! v.optformborderStyle}" onchange="{!c.handleCss}" />
                                    <lightning:input type="text" value="{!v.formborderwidth }" label="Border thickness (px)" onchange="{!c.handleCss}" class="slds-m-bottom_medium" />
                                </div>

                                <button class="accordion" onclick="{!c.designaccordian}">Label</button>
