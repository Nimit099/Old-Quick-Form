                                    <lightning:input type="color" label="Input background" value='{!v.fieldfocusbg}' onchange="{!c.handleCss}" />
                                    <lightning:input type="color" label="Input border" value='{!v.fieldfocusborderColor}' onchange="{!c.handleCss}" class="colorBarFocusInputBorder" />
                                    <lightning:input type="color" label="Input text color" value="{!v.fieldfocuscolor}" onchange="{!c.handleCss}" class="colorBarFocusInput" />
                                    <lightning:input type="color" label="Label color" value="{!v.focuslabelcolor}" onchange="{!c.handleCss}" class="slds-m-bottom_medium colorBar" />

